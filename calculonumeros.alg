algoritmo "CALCULONUMEROS"

var

n1, n2, soma: inteiro
sub,multi,div: inteiro


inicio

escreva("Escreva dois numeros: ")
leia (n1)
leia (n2)

soma <- n1 + n2
sub <- n1 - n2
multi <- n1 * n2
div <- n1 / n2


escreva("SOMA: ", soma ,",", " SUBTRACAO: ",  sub ," ," ,"MULTIPICACAO: ", multi ,"," , "DIVISAO: ", div )


fimalgoritmo